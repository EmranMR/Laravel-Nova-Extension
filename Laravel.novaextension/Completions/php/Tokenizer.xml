<?xml version="1.0" encoding="UTF-8"?>
<completions>
    <set name="php.core.functions.tokenizer">
        <completion string="token_get_all">
            <behavior symbol="function">
                <append>($[$code])</append>
            </behavior>
        </completion>
        <completion string="token_name">
            <behavior symbol="function">
                <append>($[$id])</append>
            </behavior>
        </completion>
    </set>
</completions>
