<?xml version="1.0" encoding="UTF-8"?>
<completions>
    <set name="php.core.functions.fileinfo">
        <completion string="finfo_buffer">
            <behavior symbol="function">
                <append>($[$finfo], $[$string])</append>
            </behavior>
        </completion>
        <completion string="finfo_close">
            <behavior symbol="function">
                <append>($[$finfo])</append>
            </behavior>
        </completion>
        <completion string="finfo_file">
            <behavior symbol="function">
                <append>($[$finfo], $[$filename])</append>
            </behavior>
        </completion>
        <completion string="finfo_open">
            <behavior symbol="function">
                <append>()</append>
            </behavior>
        </completion>
        <completion string="finfo_set_flags">
            <behavior symbol="function">
                <append>($[$finfo], $[$flags])</append>
            </behavior>
        </completion>
        <completion string="mime_content_type">
            <behavior symbol="function">
                <append>($[$filename])</append>
            </behavior>
        </completion>
    </set>
</completions>
