<?xml version="1.0" encoding="UTF-8"?>
<completions>
    <set name="php.core.functions.shmop">
        <completion string="shmop_close">
            <behavior symbol="function">
                <append>($[$shmop])</append>
            </behavior>
        </completion>
        <completion string="shmop_delete">
            <behavior symbol="function">
                <append>($[$shmop])</append>
            </behavior>
        </completion>
        <completion string="shmop_open">
            <behavior symbol="function">
                <append>($[$key], $[$mode], $[$permissions], $[$size])</append>
            </behavior>
        </completion>
        <completion string="shmop_read">
            <behavior symbol="function">
                <append>($[$shmop], $[$offset], $[$size])</append>
            </behavior>
        </completion>
        <completion string="shmop_size">
            <behavior symbol="function">
                <append>($[$shmop])</append>
            </behavior>
        </completion>
        <completion string="shmop_write">
            <behavior symbol="function">
                <append>($[$shmop], $[$data], $[$offset])</append>
            </behavior>
        </completion>
    </set>
</completions>
