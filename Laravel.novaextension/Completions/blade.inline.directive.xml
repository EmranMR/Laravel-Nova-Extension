        <behavior prefix="@">
            <append>($[])</append>
        </behavior>
        <completion string="@extends"/>
        <completion string="@yield"/>
        <completion string="@include"/>
        <completion string="@includeIf"/>
        <completion string="@includeWhen"/>
        <completion string="@includeUnless"/>
        <completion string="@includeFirst"/>
        <completion string="@props"/>
        <completion string="@method"/>
        <completion string="@inject"/>
        <completion string="@each"/>
        <completion string="@vite"/>
        <completion string="@livewire"/>
        <completion string="@aware"/>
        <completion string="@section"/>
    </set>
</completions>