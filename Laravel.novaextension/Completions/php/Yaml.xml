<?xml version="1.0" encoding="UTF-8"?>
<completions>
    <set name="php.core.functions.yaml">
        <completion string="yaml_emit_file">
            <behavior symbol="function">
                <append>($[$filename], $[$data])</append>
            </behavior>
        </completion>
        <completion string="yaml_emit">
            <behavior symbol="function">
                <append>($[$data])</append>
            </behavior>
        </completion>
        <completion string="yaml_parse_file">
            <behavior symbol="function">
                <append>($[$filename])</append>
            </behavior>
        </completion>
        <completion string="yaml_parse_url">
            <behavior symbol="function">
                <append>($[$url])</append>
            </behavior>
        </completion>
        <completion string="yaml_parse">
            <behavior symbol="function">
                <append>($[$input])</append>
            </behavior>
        </completion>
    </set>
</completions>
