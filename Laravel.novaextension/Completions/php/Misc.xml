<?xml version="1.0" encoding="UTF-8"?>
<completions>
    <set name="php.core.functions.misc">
        <completion string="connection_aborted">
            <behavior symbol="function">
                <append>()</append>
            </behavior>
        </completion>
        <completion string="connection_status">
            <behavior symbol="function">
                <append>()</append>
            </behavior>
        </completion>
        <completion string="constant">
            <behavior symbol="function">
                <append>($[$name])</append>
            </behavior>
        </completion>
        <completion string="define">
            <behavior symbol="function">
                <append>($[$constant_name], $[$value])</append>
            </behavior>
        </completion>
        <completion string="defined">
            <behavior symbol="function">
                <append>($[$constant_name])</append>
            </behavior>
        </completion>
        <completion string="die">
            <behavior symbol="function">
                <append>()</append>
            </behavior>
        </completion>
        <completion string="eval">
            <behavior symbol="function">
                <append>($[$code])</append>
            </behavior>
        </completion>
        <completion string="exit">
            <behavior symbol="function">
                <append>($[$status])</append>
            </behavior>
        </completion>
        <completion string="get_browser">
            <behavior symbol="function">
                <append>()</append>
            </behavior>
        </completion>
        <completion string="highlight_file">
            <behavior symbol="function">
                <append>($[$filename])</append>
            </behavior>
        </completion>
        <completion string="highlight_string">
            <behavior symbol="function">
                <append>($[$str])</append>
            </behavior>
        </completion>
        <completion string="hrtime">
            <behavior symbol="function">
                <append>()</append>
            </behavior>
        </completion>
        <completion string="ignore_user_abort">
            <behavior symbol="function">
                <append>($[$enable])</append>
            </behavior>
        </completion>
        <completion string="pack">
            <behavior symbol="function">
                <append>($[$format], $[$...$values])</append>
            </behavior>
        </completion>
        <completion string="php_strip_whitespace">
            <behavior symbol="function">
                <append>($[$filename])</append>
            </behavior>
        </completion>
        <completion string="show_source">
            <behavior symbol="function">
                <append>($[$filename])</append>
            </behavior>
        </completion>
        <completion string="sleep">
            <behavior symbol="function">
                <append>($[$seconds])</append>
            </behavior>
        </completion>
        <completion string="sys_getloadavg">
            <behavior symbol="function">
                <append>()</append>
            </behavior>
        </completion>
        <completion string="time_nanosleep">
            <behavior symbol="function">
                <append>($[$seconds], $[$nanoseconds])</append>
            </behavior>
        </completion>
        <completion string="time_sleep_until">
            <behavior symbol="function">
                <append>($[$timestamp])</append>
            </behavior>
        </completion>
        <completion string="uniqid">
            <behavior symbol="function">
                <append>()</append>
            </behavior>
        </completion>
        <completion string="unpack">
            <behavior symbol="function">
                <append>($[$format], $[$string])</append>
            </behavior>
        </completion>
        <completion string="usleep">
            <behavior symbol="function">
                <append>($[$microseconds])</append>
            </behavior>
        </completion>
        <completion string="__halt_compiler">
            <behavior symbol="function">
                <append>()</append>
            </behavior>
        </completion>
    </set>
</completions>
